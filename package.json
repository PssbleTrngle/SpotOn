    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.170",
    "@types/mongoose": "^5.10.5",
    "@types/react-virtualized": "^9.21.11",
    "axios": "^0.21.1",
    "joi": "^17.4.0",
    "lodash": "^4.17.21",
    "luxon": "^1.27.0",
    "mongodb": "^3.6.6",
    "mongoose": "^5.12.11",
    "next": "latest",
    "next-auth": "^3.23.3",
    "node-cache": "^5.1.2",
    "polished": "^4.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-tooltip": "^4.2.21",
